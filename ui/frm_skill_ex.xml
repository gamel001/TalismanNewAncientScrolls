      <Label AlignCol="CENTER" AlignLine="CENTER" Inherit="" Location="110,-5,120,68" Name="6102" Show="false" Text="$L:171$" TextFont="$L:1$" TextSize="$L:3$"/>
      <Scripts>
        <OnShow>uiRegisterEscWidget(self)</OnShow>
        <OnLoad>self:RegisterScriptEventNotify("ToggleMagicWeapon")
				self:RegisterScriptEventNotify("EVENT_SelfEnterWorld")
				self:RegisterScriptEventNotify("UserSkillRefresh")

				self:RegisterScriptEventNotify("equipment item changed")
				self:RegisterScriptEventNotify("equipment item equiped")
				self:RegisterScriptEventNotify("equipment item unequiped")</OnLoad>
        <OnEvent>if event == "ToggleMagicWeapon" then
				if (arg[1] ~= EV_EXCUTE_EVENT_KEY_DOWN) and (arg[1] ~= EV_EXCUTE_EVENT_ON_LCLICK) then
					return
				end
				layWorld_frmSkillEx_Show(self)
			elseif event == "EVENT_SelfEnterWorld" then
				
				layWorld_frmSkillEx_Player_EnterWorld()
			elseif event == "UserSkillRefresh" then
				if not self:getVisible() then return end
				layWorld_frmSkillEx_Refresh()
			elseif event == "equipment item changed" then
				if not self:getVisible() then return end
				layWorld_frmSkillEx_Refresh()
			elseif event == "equipment item equiped" then
				if not self:getVisible() then return end
				layWorld_frmSkillEx_Refresh()
			elseif event == "equipment item unequiped" then
				if not self:getVisible() then return end
				layWorld_frmSkillEx_Refresh()
			end</OnEvent>
        <OnUpdate Timer="40">layWorld_frmSkillEx_OnUpdate(self)</OnUpdate>
      </Scripts>
    </Form>
